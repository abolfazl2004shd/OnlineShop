@page
@model OnlineShop.Areas.Admin.Pages.Product.IndexModel
@{
}


<table class="table table-striped table-dark table-bordered">
    <tr>
        <th>ProductName</th>
        <th>Color</th>
        <th>Size</th>
        <th>Amount</th>

    </tr>

    @foreach (var product in Model.Products)
    {
        <tr>
            <th>@product.ProductName</th>
            <th>@product.Color</th>
            <th>@product.Size</th>
            <th>@product.Amount</th>

        </tr>
    }
</table>

<a class="btn btn-success" asp-area="Customer" asp-page="/Product/Add">Add Product</a>