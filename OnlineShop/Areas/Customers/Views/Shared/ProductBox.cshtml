@model Product
<div class="card col-4" style="width: 18rem;margin:20px;cursor:pointer;">
    <img class="card-img-top" src="data:image/jpeg;base64,">
    <div class="card-body">
        <h5 class="card-title">@Model.ProductName</h5>
        <h5 class="card-title">@Model.Branch.Shop.Name - @Model.Branch.BranchName</h5>
        <p class="card-text" style="color:cornflowerblue;">@Model.Price $</p>
        @*         <a asp-area="Client" asp-controller="Order" asp-action="" asp-route-id=@Model.ProductId class="btn btn-primary">Buy</a>
        *@
    </div>
</div>
<script>
    var cards = document.querySelectorAll('div.card');
    cards.forEach(e => {
        e.addEventListener('click', () => {
            window.open("/Customers/Products/Details/@Model.ProductId");
        });
        e.addEventListener('mouseenter', () => {
            e.style.backgroundColor = "skyblue";
        });
        e.addEventListener('mouseleave', () => {
            e.style.backgroundColor = "white";
        });
    });

</script>